{
  "name": "typescript-nextjs-starter",
  "description": "A TypeScript starter for Next.js that includes all you need to build amazing projects",
  "version": "1.0.0",
  "private": true,
  "author": "Jo√£o Pedro Schmitz <oi@joaopedro.cc> (joaopedro.cc)",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn format"
    ]
  },
  "dependencies": {
    "next": "9.5.3",
    "normalize.css": "^8.0.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "request-ip": "^2.1.3",
    "tldts": "^5.6.59",
    "use-http": "^1.0.15"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@mdx-js/loader": "1.6.18",
    "@next/mdx": "9.5.3",
    "@types/mdx-js__react": "^1.5.3",
    "@types/node": "14.11.2",
    "@types/react": "16.9.49",
    "@types/react-dom": "16.9.8",
    "@types/request-ip": "^0.0.35",
    "husky": "4.3.0",
    "lint-staged": "10.4.0",
    "next-compose-plugins": "2.2.0",
    "prettier": "2.1.2",
    "typescript": "4.0.3"
  }
}
